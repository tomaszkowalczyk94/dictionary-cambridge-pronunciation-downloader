{
  "manifest_version": 2,

  "name": "dictionary cambridge pronunciation downloader",
  "description": "dictionary cambridge pronunciation downloader",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "activeTab"
  ],
  
  "content_scripts": [
    {
      "matches": ["https://dictionary.cambridge.org/*", "https://www.diki.pl/*"],
      "js": ["jquery-3.2.1.min.js", "content.js"]
    }
  ],
  "web_accessible_resources": [
    "download.png"
  ]
  
}
